<?xml version="1.0" encoding="UTF-8"?> 
<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm" 
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	xsi:schemaLocation="http://java.sun.com/xml/ns/persistence/orm http://java.sun.com/xml/ns/persistence/orm_1_0.xsd" 
	version="1.0"> 
    <entity class="es.mdef.apigatel.entidades.UnidadConId" access="FIELD">
		<table name="UNIDADES"/>
        <attributes> 
            <id name="id"> 
                <generated-value strategy="IDENTITY"/> 
            </id> 
            <basic name="nombre" optional="false">
               <column name="nombre"/>
            </basic>
            <basic name="codigoUnidad" optional="false">
               <column name="codigoUnidad"/>
            </basic>
            <basic name="correoOficial" optional="false">
               <column name="correoOficial"/>
            </basic>
            <basic name="telefono" optional="false">
               <column name="telefono"/>
            </basic>
          </attributes>
     </entity>
</entity-mappings>